{
  "name": "test-rail-sync",
  "displayName": "TestRail Sync",
  "description": "TestRail Sync extension",
  "author": "MutantX",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension/index.js",
  "contributes": {
    "commands": [
      {
        "command": "get-suites",
        "title": "1. Get all suites"
      },
      {
        "command": "get-test-case",
        "title": "2. Get test case"
      },
      {
        "command": "get-test-cases",
        "title": "3. Get all test cases"
      },
      {
        "command": "create-test-case",
        "title": "4. Create new test case"
      },
      {
        "command": "create-test-cases",
        "title": "5. Create all new test cases"
      },
      {
        "command": "update-test-case",
        "title": "6. Update test case"
      },
      {
        "command": "update-test-cases",
        "title": "7. Update all test cases"
      },
      {
        "command": "delete-test-case",
        "title": "8. Delete test case"
      },
      {
        "command": "delete-test-cases",
        "title": "9. Delete all test cases"
      }
    ],
    "submenus": [
      {
        "id": "submenu",
        "label": "TestRail Sync"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "group": "test-rail-sync",
          "submenu": "submenu",
          "when": "editorLangId == typescript"
        }
      ],
      "submenu": [
        {
          "command": "get-suites",
          "group": "1_submenu"
        },
        {
          "command": "get-test-case",
          "group": "1_submenu"
        },
        {
          "command": "get-test-cases",
          "group": "1_submenu"
        },
        {
          "command": "create-test-case",
          "group": "2_submenu"
        },
        {
          "command": "create-test-cases",
          "group": "2_submenu"
        },
        {
          "command": "update-test-case",
          "group": "3_submenu"
        },
        {
          "command": "update-test-cases",
          "group": "3_submenu"
        },
        {
          "command": "delete-test-case",
          "group": "4_submenu"
        },
        {
          "command": "delete-test-cases",
          "group": "4_submenu"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "rm -rf dist && webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --fix",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/axios": "^0.14.4",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/vscode": "^1.94.0",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.13.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest-dom": "^5.4.0",
    "eslint-plugin-no-unsanitized": "^4.1.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-testing-library": "^6.4.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "prettier": "^3.3.3",
    "source-map-loader": "^5.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.3",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  },
  "dependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@babel/preset-typescript": "^7.26.0",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.6",
    "@mui/material": "^6.1.5",
    "@mui/x-data-grid": "^7.22.1",
    "axios": "^1.7.7",
    "babel-loader": "^9.2.1",
    "http-proxy-middleware": "^3.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
